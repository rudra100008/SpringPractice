<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean class="com.SpringCore.Spring.lifecycle.Animal" name="animal"  init-method="init" destroy-method="destroy">
  <property  name="name" value="Dog"  />
  <property  name="species" value="Bulldog"/>
  <property name="age" value="10"/>
</bean>
<bean class="com.SpringCore.Spring.lifecycle.Animal1" name="animal1" >
<property  name="name" value="Tiger"  />
  <property  name="species" value="BengalTiger"/>
  <property name="age" value="20"/>
</bean>

</beans>









